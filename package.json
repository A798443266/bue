{
	"name": "bowen-bue",
	"version": "0.0.2",
	"description": "",
	"unpkg": "dist/bue.min.js",
	"main": "dist/bue.js",
	"scripts": {
		"start": "npm run dev",
		"dev": "webpack-dev-server --progress --config build/webpack.config.dev.js",
		"build": "webpack --config build/webpack.config.prod.js && webpack --config build/webpack.config.doc.js",
		"prettier": "prettier --write ./src/**/* && prettier --write ./example/**/*",
		"commitmsg": "commitlint -E GIT_PARAMS",
		"precommit": "lint-staged",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"lint-staged": {
		"**/*.{js,ts}": [
			"prettier --write",
			"git add"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/bowencool/bue.git"
	},
	"keywords": [
		"mvvm"
	],
	"author": "bowencool",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/bowencool/bue/issues"
	},
	"homepage": "https://github.com/bowencool/bue#readme",
	"devDependencies": {
		"@commitlint/cli": "^7.0.0",
		"@commitlint/config-angular": "^7.0.1",
		"html-webpack-plugin": "^3.2.0",
		"husky": "^0.14.3",
		"lint-staged": "^7.2.0",
		"prettier": "^1.13.7",
		"webpack": "^4.16.0",
		"webpack-cli": "^3.0.8",
		"webpack-dev-server": "^3.1.4",
		"webpack-merge": "^4.1.3"
	}
}
